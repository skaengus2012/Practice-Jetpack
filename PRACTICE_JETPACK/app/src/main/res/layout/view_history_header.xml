<?xml version="1.0" encoding="utf-8"?>
<layout>
	<data>
		<variable name="viewModel" type="nlab.practice.jetpack.ui.common.viewmodel.SimpleItemViewModel"/>
	</data>

	<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="@dimen/history_page_header_height"
		android:paddingStart="@dimen/history_page_item_margin_start_end"
		android:paddingEnd="@dimen/history_page_item_margin_start_end">

		<TextView
			android:id="@+id/tvHomeTitle"
			style="@style/MainContainerTitle"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/history_title"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toStartOf="@id/tvHomeSubTitle"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent"/>

	</androidx.constraintlayout.widget.ConstraintLayout>
</layout>